/* 阿里巴巴矢量图标库 */
@font-face {
  font-family: 'iconfont';
  src: url('https://at.alicdn.com/t/font_1458415_qig7givgr7.ttf') format('truetype');
}
.iconfont { font-family: 'iconfont';
}
/* 页面主体 */
page {
  color: #333333;
  font-size: 32rpx;
}
/* 统一所有元素的盒子模型 */
page, 
view, 
scroll-view, 
swiper, 
movable-area, 
movable-view, 
cover-view, 
cover-image, 
icon, 
text, 
rich-text, 
progress, 
burron, 
checkbox, 
editor, 
form, 
input, 
label, 
picker, 
picker-view, 
radio, 
slider, 
switch, 
textarea, 
navigator, 
audio, 
camera, 
image, 
video, 
live-player, 
live-pusher, 
map, 
canvas, 
web-view, 
ad, 
official-account, 
open-data, 
barcode, 
list, 
cell, 
recycle-list, 
waterfall, 
resresh { -webkit-box-sizing: border-box; box-sizing: border-box;
}
